local a=loadstring(game:HttpGet('https://raw.githubusercontent.com/shlexware/Orion/main/source'))()local b=game:GetService("Players")local c=game:GetService("RunService")local d=b.LocalPlayer;local e=30;local f=false;local function g(h,i,j)local k,l=pcall(function()firetouchinterest(h,i,j)end)if not k then warn("Error interacting with part:",i.Name,l)end end;local function m(n)local o=n.Character;local p=o and o:FindFirstChildOfClass("Humanoid")return p and p.Health>0 and o:FindFirstChild("HumanoidRootPart")and d:DistanceFromCharacter(o.HumanoidRootPart.Position)<=e end;local function q(n)if m(n)then local r=d.Character and d.Character:FindFirstChildOfClass("Tool")if r and r:FindFirstChild("Handle")then r:Activate()for s,i in ipairs(n.Character:GetDescendants())do if i:IsA("BasePart")then g(r.Handle,i,0)g(r.Handle,i,1)end end end end end;local function t(u)f=u;print("Reach script is now:",f and"Enabled"or"Disabled")if f then while f do for s,v in ipairs(b:GetPlayers())do if v~=d then q(v)end end;wait()end end end;local w=a:MakeWindow({Name="Unpaid Reach DM 9dots. to get Paid version",HidePremium=false,SaveConfig=true,ConfigFolder="ScriptControlsConfig"})local x=w:MakeTab({Name="Reach",Icon="rbxassetid://4483345998",PremiumOnly=false})local y=x:AddSlider({Name="Reach",Min=1,Max=100,Default=30,Increment=1,ValueName=" studs",Callback=function(z)e=z;print("Reach set to:",z)end})local A=x:AddToggle({Name="Toggle Reach Script",Default=false,Callback=t})local B=w:MakeTab({Name="Scripts",Icon="rbxassetid://4483345998",PremiumOnly=false})B:AddButton({Name="Execute Script 1",Callback=function()loadstring(game:HttpGet('https://gist.github.com/inuk84/ca841931f45f242c945405298b26bd71/raw/af364e5d4ea5b0d2eac404a5376d83c84a23cbe4/na.lua'))()end})B:AddButton({Name="Execute Script 2",Callback=function()loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()end})a:Init()
